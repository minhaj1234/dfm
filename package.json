    "karma-threshold-reporter": "^0.1.15",
    "ng-packagr": "^5.5.1",
    "npm-run-all": "^4.1.5",
    "protractor": "^5.4.2",
    "rxjs-tslint": "0.1.5",
    "ts-node": "^8.3.0",
    "tsickle": "^0.36.0",
    "tslib": "^1.9.0",
