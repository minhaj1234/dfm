{
  "name": "dfm",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "ng-high-memory": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng",
    "start:modeler": "yarn run build:core && yarn run build:user-management && ng serve --project modeler",
    "start:admin": "yarn run build:core && yarn run build:user-management && ng serve --project admin",
    "build:modeler": "ng build --project modeler",
    "build:admin": "ng build --project admin",
    "build:core": "yarn run ng-high-memory build --project core",
    "build:user-management": "ng build --project user-management",
    "precommit": "run-s lint",
    "prepush": "yarn run build:core && yarn run build:user-management && yarn test:ci",
    "test": "yarn run ng test --code-coverage --browsers=Chrome",
    "test:ci": "yarn run ng test --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "analyze:modeler": "yarn run build:core && yarn run build:user-management && ng build --prod --project modeler --stats-json && webpack-bundle-analyzer dist/modeler/stats.json",
    "analyze:admin": "yarn run build:core && yarn run build:user-management && ng build --prod --project admin --stats-json && webpack-bundle-analyzer dist/admin/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.3",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "~8.2.3",
    "@angular/compiler": "~8.2.3",
    "@angular/core": "~8.2.3",
    "@angular/forms": "~8.2.3",
    "@angular/platform-browser": "~8.2.3",
    "@angular/platform-browser-dynamic": "~8.2.3",
    "@angular/router": "~8.2.3",
    "@nebular/theme": "~4.2.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/entity": "^8.2.0",
    "@ngrx/router-store": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngrx/store-devtools": "^8.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@stomp/ng2-stompjs": "^7.2.0",
    "@types/deep-diff": "^0.0.31",
    "@types/dompurify": "^2.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/xregexp": "^3.0.30",
    "angular-resizable-element": "^3.2.4",
    "angular4-hal": "^7.8.14",
    "auth0-js": "9.11.3",
    "core-js": "^3.2.1",
    "deep-diff": "^1.0.1",
    "dompurify": "^2.0.7",
    "eva-icons": "^1.1.1",
    "gojs": "^1.8.38",
    "guid-typescript": "^1.0.9",
    "hammerjs": "^2.0.8",
    "jasmine-auto-spies": "^5.0.2",
    "jwt-decode": "^2.2.0",
    "keycode-js": "^2.0.3",
    "nebular-icons": "^1.1.0",
    "ng-drag-drop": "^5.0.0",
    "ngrx-store-localstorage": "^8.0.0",
    "ngx-clipboard": "^12.2.1",
    "ngx-contextmenu": "^5.2.0",
    "ngx-print": "1.2.0-beta.3",
    "ngx-quill": "^5.2.0",
    "ngx-take-until-destroy": "^5.4.0",
    "primeng": "^8.0.2",
    "quill": "^1.3.6",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.2",
    "sockjs-client": "^1.3.0",
    "web-animations-js": "^2.3.2",
    "xregexp": "^4.2.4",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.0",
    "@angular-devkit/build-ng-packagr": "0.800.0",
    "@angular-devkit/core": "^8.3.0",
    "@angular-devkit/schematics": "^8.3.0",
    "@angular/cli": "^8.3.0",
    "@angular/compiler-cli": "~8.2.3",
    "@angular/language-service": "~8.2.3",
    "@netbasal/spectator": "^3.9.4",
    "@ngrx/schematics": "^8.2.0",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.7.2",
    "codelyzer": "^5.1.0",
    "husky": "^3.0.4",
    "jasmine-core": "^3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~3.1.0",
